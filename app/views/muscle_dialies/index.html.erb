<div class="page-header">
  <%= link_to new_muscle_dialy_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Muscle dialy
  <% end %>
  <h1>Listing muscle_dialies</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @muscle_dialies) do |muscle_dialy| %>
            <td><%= link_to 'Show', muscle_dialy %></td>
        <td><%= link_to 'Edit', edit_muscle_dialy_path(muscle_dialy) %></td>
        <td><%= link_to 'Destroy', muscle_dialy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
